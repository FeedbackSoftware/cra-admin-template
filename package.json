{
  "name": "@fbacks/cra-template-fbacks-admin",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Angel Garcia <angelgarcia@fbacks.com",
  "description": "A quick start Create React App template with React Admin, Keycloak, and MixPanel",
  "main": "template.json",
  "files": [
    "template",
    "template.json"
  ],
  "keywords": [
    "react",
    "create-react-app",
    "cra-template",
    "template",
    "keycloak",
    "admin",
    "redux",
    "react-admin",
    "mixpanel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FeedbackSoftware/cra-admin-template",
    "directory": "/"
  },
  "dependencies": {
    "@react-keycloak/web": "^2.1.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "keycloak-js": "^10.0.2",
    "prop-types": "^15.7.2",
    "ra-data-json-server": "^3.8.3",
    "react": "^16.13.1",
    "react-admin": "^3.8.3",
    "react-dom": "^16.13.1",
    "react-mixpanel-browser": "^3.1.1",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean-files": "rm -rf ./template/public ./template/src && rm -f ./template/.eslintrc",
    "copy-files": "cp -a ./src/. template/src && cp -a ./public/. template/public && cp .eslintrc template/",
    "prepublishOnly": "yarn clean-files && yarn copy-files"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
